{{#each feedbacks }}
    <tr>
        <td>{{ increment @index }}</td>
        <td>{{ this.name }}</td>
        <td>{{ this.email }}</td>
        <td>{{ this.phone }}</td>
        <td>
            <a href="feedback_view.html?id={{ this._id }}" class="btn btn-primary"><i
                class="fa fa-eye fa-lg"
                aria-hidden="true"></i>View</a>
            <a href="javascript:void(0);" data-id="{{this._id}}" class="btn btn-danger"
               onclick="return confirm('Are you sure to delete this record ?');"><i
                    class="fa fa-trash fa-lg" aria-hidden="true"></i>Delete</a>
        </td>
    </tr>
{{/each}}
